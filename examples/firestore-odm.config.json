{
  "schema": "./firestore.schema.json",
  "outputs": [
    {
      "language": "typescript",
      "outputDir": "../generated/ts",
      "target": "typescript-client",
      "package": {
        "name": "@my-project/firestore-models-ts",
        "version": "0.1.0",
        "description": "Generated TS models for Firestore"
      },
      "options": {
        "dateTimeType": "Timestamp"
      }
    }
,
    {
      "language": "dart",
      "outputDir": "../generated/dart",
      "target": "dart-client",
      "package": {
        "name": "my_project_firestore_models_dart",
        "version": "0.1.0",
        "description": "Generated Dart models for Firestore"
      },
      "options": {
        "nullSafety": true
      }
    }

  ],
  "generatorOptions": {
    "logLevel": "info"
  }
}